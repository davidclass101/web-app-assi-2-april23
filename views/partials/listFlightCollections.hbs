<div class="ui centered cards">
  {{#each flightCollections}}
    <div class="ui card">
      <div class="image">
        <img src="{{picture}}" />
      </div>
      <div class="content">
        <a class="header">{{title}}</a>
      </div>
      <div class="extra content">
        <a href="/flightCollection/{{id}}" class="ui icon button">
          <i class="icon folder open"></i>
        </a>
        <a
          href="/dashboard/deleteFlightCollection/{{id}}"
          class="ui icon button delFlightCollection"
        >
          <i class="icon trash"></i>
        </a>
      </div>
    </div>
  {{/each}}
</div>